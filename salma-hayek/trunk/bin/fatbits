#!/bin/bash
real_script=`readlink $0 || echo $0` # Deals with symbolic links to this script.
home=`dirname $real_script`/..
if [ "`uname`" == "Darwin" ];
then
 dock_name=-Xdock:name=FatBits
 dock_icon='-Xdock:icon=/Developer/Applications/Graphics Tools/Pixie.app/Contents/Resources/Big.icns'
fi
java "$dock_name" "$dock_icon" -Xmx1g -cp $home/classes e.tools.FatBits "$@"
