#!/usr/bin/ruby -w

require 'fileutils.rb'
require 'pathname.rb'

# Cope with symbolic links to this script.
salma_hayek = Pathname.new("#{__FILE__}/..").realpath().dirname()

require "#{salma_hayek}/bin/invoke-java.rb"

app_name="Notepad"
class_name="e.tools.notepad.Notepad"

invoker = Java.new(app_name, "e/util/Launcher")
invoker.invoke([ app_name, class_name ])
