#!/bin/bash
real_script=`readlink $0 || echo $0`
class_path=`dirname $real_script`/../classes

app_name=Notepad
class_name=e.tools.notepad.Notepad

shift
case "`uname`" in
Darwin)
    os_specific_options=-Xdock:name=$app_name
    ;;
*)
    class_path=$class_path:$tools_dir/../salma-hayek/MRJ141Stubs.jar
    ;;
esac

exec java $os_specific_options -Xmx1g -cp $class_path e.util.Launcher $app_name $class_name "$@"
