# Terminator options file for Mac OS X.

Highlighter WebHighlighter {
	openInTab = no
	regexp = "(\b(http|https|ftp):/*[^\s:\"]+(:\d+)?[/\w\.\?&=\+]*)"
	highlightGroup = 1
	command = "open $1"
}

Highlighter FileHighlighter {
	openInTab = no
	regexp = "(?:^| |")(/[^ :"]+\.\w+([\d:]+)?)"
	highlightGroup = 1
	command = "vi $1"
}
